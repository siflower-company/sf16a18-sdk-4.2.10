
config wifi-device 'radio0'
	option type 'mac80211'
	option band '2.4G'
	option max_all_num_sta '40'
	option netisolate '0'
	option country 'CN'
	option ht_coex '0'
	option noscan '0'
	option radio '1'
	option path '10000000.palmbus/11000000.wifi-lb'
	option htmode 'HT20'
	option hwmode '11g'
	option channel '1'
	option beacon_int 200

config wifi-iface
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option encryption 'open'
	option isolate '0'
	option hidden '0'
	option macfilter 'disable'
	option macfile '/etc/wlan-file/wlan0.allow'
	option group '1'
	option netisolate '1'
	option disable_input '0'
	option ssid 'SiWiFi-repeater'
	option key '12345678'
	option rps_cpus '2'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '161'
	option band '5G'
	option max_all_num_sta '40'
	option netisolate '0'
	option country 'CN'
	option ht_coex '0'
	option noscan '1'
	option radio '1'
	option path '10000000.palmbus/11400000.wifi-hb'
	option htmode 'VHT80'
	option hwmode '11a'
	option beacon_int 200

config wifi-iface
	option device 'radio1'
	option ifname 'wlan1'
	option network 'lan'
	option mode 'ap'
	option encryption 'open'
	option isolate '0'
	option hidden '0'
	option macfilter 'disable'
	option macfile '/etc/wlan-file/wlan1.allow'
	option group '1'
	option netisolate '1'
	option disable_input '0'
	option ssid 'SiWiFi-repeater-5G'
	option key '12345678'
	option rps_cpus '3'

config wifi-iface
	option key '88886666jkllkj'
	option ifname 'sfi0'
	option network 'wwan'
	option encryption 'psk2+ccmp'
	option device 'radio0'
	option mode 'sta'
	option bssid 'A8:5A:F3:00:30:54'
	option ssid 'SiWiFi-test'
	option rps_cpus '2'
	option disabled '0'

config wifi-iface
	option key '88886666jkllkj'
	option ifname 'sfi1'
	option network 'wwwan'
	option encryption 'psk2+ccmp'
	option device 'radio1'
	option mode 'sta'
	option bssid 'A8:5A:F3:00:30:58'
	option ssid 'SiWiFi-test-5G'
	option rps_cpus '3'
	option disabled '0'
